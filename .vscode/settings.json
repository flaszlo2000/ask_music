{
    "files.exclude": {
        "package-lock.json": true,
        "package.json": true,
        "karma.conf.js": true,
        ".browserslistrc": true,
        ".editorconfig": true,
        ".gitignore": true,
        "README.md": true,
        "tsconfig.*": true,
        "node_modules": true,
        ".vscode": true,
        "src/assets/": true,
        "src/favicon.ico": true,
        "src/main.ts": true,
        "src/polyfills.ts": true,
        "src/test.ts": true,
        "src/app/**/*.component.spec.ts": true,
        "src/app/**/*.service.spec.ts": true,
        ".angular": true,
    },
    "terminal.integrated.cwd": "./src/app",
    "editor.acceptSuggestionOnCommitCharacter": false,
}
